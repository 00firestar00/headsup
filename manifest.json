{
  "manifest_version": 2,

  "name": "ModHeader",
  "description": "",
  "version": "0.0.1",
  "icons": {
    "16": "img/i16.png",
    "48": "img/i48.png",
    "128": "img/i128.png"
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "19": "img/i48.png",
      "38": "img/i48.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "storage",
    "webRequest",
    "webRequestBlocking"
  ],
  "web_accessible_resources": ["img/i128.png"]
}